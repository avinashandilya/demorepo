<?xml version="1.0" encoding="UTF-8"?>

<!-- Write a TestScript to execute Multiple Packages in Maven -->

<suite parallel="false" name="Suite">
  <test name="Test">
    <packages>
    <package name = "browser.Chrome"/>
    <package name = "browser.Edge"/>
    <package name = "browser.Firefox"/>
    </packages>
  </test> <!-- Test -->
</suite> <!-- Suite -->
